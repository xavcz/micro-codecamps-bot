{
  "name": "micro-codecamps-bot",
  "version": "1.0.0",
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon --config package.json src/index.js",
    "prestart": "npm run build",
    "start": "micro dist/index.js",
    "build": "./node_modules/.bin/async-to-gen src --out-dir dist",
    "lint": "eslint --quiet --ext src/*.js .",
    "prettier": "prettier --write \"src/*.js\" --single-quote=true --trailing-comma=es5 --print-width=100",
    "test": "npm run lint",
    "deploy": "now -E .env"
  },
  "repository": "https://github.com/xavcz/micro-codecamps-bot.git",
  "author": "xavcz <xavier.cazalot@gmail.com> (http://xav.cz)",
  "license": "MIT",
  "dependencies": {
    "micro": "^7.0.6",
    "node-schedule": "^1.2.0",
    "promisify-node": "^0.4.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "urlencoded-body-parser": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.16.1",
    "eslint-plugin-prettier": "^2.0.1",
    "prettier": "^0.20.0"
  },
  "execMap": {
    "js": "micro"
  }
}
